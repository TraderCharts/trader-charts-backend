{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ],
  "env": {
    "build": {
      "comments": false, // opcional: remover comentarios en build
      "minified": true // antes usabas optional: ["optimisation", "minification"]
    }
  }
}
